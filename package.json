  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js": {
      "style": "module"
    }
  },
  "devDependencies": {
    "@types/mjml-browser": "^4.15.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "postcss-custom-properties": "^14.0.1",
    "postcss-import": "^16.1.0"
